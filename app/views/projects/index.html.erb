<h1>Listing projects</h1>


<% @projects.each do |project| %>
	<div class="project">
	  	<li class="title"><%= project[:title] %></li>
	  	<li class="title"> Teaser: <%= project.teaser %> </li>
		<li class="title"> Description: <%= project.description %> </li>
		<li class="title"> Goal: $<%= project.goal.to_f %> </li>
	</div>
	<br>
<% end %>
